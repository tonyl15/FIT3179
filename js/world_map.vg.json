{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "title": "Estimated Population in Australia by Country of Birth, 2024",
  "width": 800,
  "height": 450,
  "projection": {"type": "equalEarth"},
  "data": {
    "url": "js/ne_110m_admin_0_countries.json",
    "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
  },
  "transform": [
    {
      "lookup": "properties.NAME",
      "from": {
        "data": {
          "url": "data/Birth by country in Australia.csv"
        },
        "key": "Country of birth",
        "fields": ["2024"]
      }
    }
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "2024",
      "type": "quantitative",
      "title": "Population",
      "scale": {"type": "log"}
    },
    "tooltip": [
      {"field": "properties.NAME", "type": "nominal", "title": "Country"},
      {"field": "2024", "type": "quantitative", "title": "Population", "format": ","}
    ]
  }
}
